<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.linux.xbmcwicd"
       name="XBMC WICD (Network Managment)"
       version="0.1"
       provider-name="cpyarger">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.script"
             library="default.py" />
  <extension point="xbmc.addon.metadata">
    <language>en</language>
    <platform>linux</platform>
    <summary lang="en">Xbmc WICD front-end. Configures wifi from within xbmc.</summary>
    <description lang="en">Xbmc WICD front-end. Configures wifi from within xbmc. Requires WICD</description>
  </extension>
</addon>
